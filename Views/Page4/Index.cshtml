@{
  ViewData["Title"] = "Kundlista";
  Layout = "_ContentNavbarLayout"; // Use your layout path
}




<div class="container-xxl flex-grow-1 container-p-y">
  <h4 class="fw-bold py-3 mb-4">Kundlista</h4>

  <!-- Stats Cards -->
  <div class="row mb-4">
    <!-- Customers Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Kunder</h5>
            <h3 class="mb-1">143</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-primary d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-group bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- Not Satisfied Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Inte nöjd</h5>
            <h3 class="mb-1">40</h3>
            <small>Total Number</small>
          </div>
          <div class="avatar-initial rounded bg-label-danger d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-angry bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- Satisfied Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Nöjd</h5>
            <h3 class="mb-1">46</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-success d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-happy-alt bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- No Record Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Ingen post</h5>
            <h3 class="mb-1">57</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-secondary d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-question-mark bx-lg"></i>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Filters -->
  <div class="card mb-4">
    <div class="card-body">
      <form class="row g-3">
        <div class="col-md-4">
          <select class="form-select">
            <option selected>Välj klassificering</option>
            <option>A Kund</option>
            <option>B Kund</option>
            <option>C Kund</option>
            <option>D Kund</option>
            <option>Inte aktiv</option>
            <option>Utsikt</option>
          </select>
        </div>
        <div class="col-md-4">
          <select class="form-select">
            <option selected>Välj nöjdhetsnivå</option>
            <option>Nöjd</option>
            <option>Inte nöjd</option>
            <option>Ingen post</option>
          </select>
        </div>
        <div class="col-md-4">
          <select class="form-select">
            <option selected> Välj utvärderingsdatum</option>
            <option>Senaste 3 månaderna</option>
            <option>Senaste 6 månaderna</option>
            <option>I år</option>
            <option>Förra året</option>
            <option>Senaste 3 åren</option>
            <option>Alla år </option>
            <option>Inget utvärderingsdatum </option>
          </select>
        </div>
      </form>
    </div>
  </div>

